  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-antrun-plugin</artifactId>
        <executions>
          <execution>
            <goals>
              <goal>run</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <tasks>
            <property name="runtime_classpath" refid="maven.runtime.classpath"/>
            <echo message="runtime classpath: ${runtime_classpath}"/>
            <java dir="${basedir}"
                  classname="com.aimir.fep.schedule.task.McuTimeSyncTask"
                  fork="true"
                  maxmemory="2048M"
                  >
              <jvmarg value="-server"/>
              <jvmarg value="-Xmx2048m"/>
              <jvmarg value="-Xms512m"/>
              <jvmarg value="-XX:PermSize=512m"/>
              <jvmarg value="-XX:MaxPermSize=512m"/>
              <jvmarg value="-XX:+UseParNewGC" />
              <jvmarg value="-XX:+UseConcMarkSweepGC" />
              <jvmarg value="-XX:-CMSParallelRemarkEnabled" />
              <!--<jvmarg value="-agentpath:/home/aimir/jprofiler6/bin/linux-x86/libjprofilerti.so=port=8849"/>-->
              <sysproperty key="name" value="McuTimeSyncTask"/>
              <sysproperty key="file.encoding" value="UTF-8"/>
              <sysproperty key="log4j.configuration" value="log4j-mcutimesync.properties"/>
              <classpath>
                <pathelement path=".:${runtime_classpath}"/>
              </classpath>
            </java>
          </tasks>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.felix</groupId>
        <artifactId>maven-bundle-plugin</artifactId>
        <version>${maven-bundle-plugin.version}</version>
        <extensions>true</extensions>
      </plugin>
    </plugins>
  </build>
    
