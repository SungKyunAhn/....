package com.aimir.fep.protocol.fmp.common;

/**
 * this class is representd for target information to access over GSM GSM
 * 
 * @author D.J Park (dong7603@nuritelecom.com)
 * @version $Rev: 1 $, $Date: 2005-11-21 15:59:15 +0900 $,
 */
public class CircuitTarget extends Target
{
    private String mobileId = null; 
    private String ipaddr = null;

    /**
     * constructor
     */
    public CircuitTarget(String mobileId,String ipaddr, String groupNumber, String memberNumber)
    {
        this.mobileId = mobileId;
        this.ipaddr = ipaddr;
        this.groupNumber = groupNumber;
        this.memberNumber = memberNumber;
    }

    /**
     * get Mobile ID(Phone Number)
     *
     * @return mobileId <code>String</code> mobile id(phone number)
     */
    public String getMobileId()
    {
        return this.mobileId;
    }

    /**
     * set Mobile ID(Phone Number)
     *
     * @param mobileId <code>String</code> mobile id(phone number)
     */
    public void setMobileId(String mobileId)
    {
        this.mobileId = mobileId;
    }

    /**
     * get IP Address
     *
     * @return ipaddr IP Address()
     */
    public String getIpaddr()
    {
        return this.ipaddr;
    }

    /**
     * set IP Address
     *
     * @param ipaddr <code>String</code> mobile id(phone number)
     */
    public void setIpaddr(String ipaddr)
    {
        this.ipaddr = ipaddr;
    }
}
